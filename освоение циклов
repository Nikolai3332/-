string massage;
int numberRepetitions;

Console.Write("Введите ваше сообщение: ");
massage = Console.ReadLine();
Console.Write("Введите количество повторов: ");
numberRepetitions = Convert.ToInt32(Console.ReadLine());

for (int i = 0; i < numberRepetitions; i++) 
Console.WriteLine($"{massage}");
Console.ReadKey();
